{
    "name": "misantron/letsencrypt-client",
    "description": "",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5",
        "psr/log": "^1.1",
        "spatie/enum": "^2.3",
        "webmozart/assert": "^1.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mikey179/vfsstream": "^1.6",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LetsEncrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LetsEncrypt\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tests": "php vendor/bin/phpunit --testsuite unit --verbose",
        "code-sniffer": "php vendor/bin/phpcs --standard=PSR2 src tests",
        "code-style": "php vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --diff"
    }
}
